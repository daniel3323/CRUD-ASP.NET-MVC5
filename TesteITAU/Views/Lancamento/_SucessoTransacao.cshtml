@model TesteITAU.Models.Lancamento

@{
    ViewBag.Title = "Sucesso Transação";
}


@using (Html.BeginForm("_SucessoTransacao", "Lancamento", FormMethod.Get))
{
    @Html.AntiForgeryToken()

<div class="container">
    <div class="row">
        <h2>Transação realizada com sucesso!</h2>
    </div>
    <div class="row">
        <h3>Clique @Html.ActionLink("AQUI", "Extrato", "Lancamento") para ser redirecionado a tela do Extrato.</h3>
    </div> 

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
</div>
}


