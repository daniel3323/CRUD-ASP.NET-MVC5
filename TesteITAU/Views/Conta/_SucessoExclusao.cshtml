@model TesteITAU.Models.Conta

@{
    ViewBag.Title = "Sucesso Transação";
}


@using (Html.BeginForm("_SucessoExclusao", "Conta", FormMethod.Get))
{
    @Html.AntiForgeryToken()

<div class="container">
    <div class="row">
        <h2>Exclusão realizada com sucesso!</h2>
    </div>
    <div class="row">
        <h3>Clique @Html.ActionLink("AQUI", "CriarConta", "Conta") para ser redirecionado.</h3>
    </div> 

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
</div>
}


