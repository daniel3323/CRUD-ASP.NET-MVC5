@model TesteITAU.Models.Usuario

@{
    ViewBag.Title = "Sucesso Alteracao";
}


@using (Html.BeginForm("_SucessoAlteracao", "Usuario", FormMethod.Get))
{
    @Html.AntiForgeryToken()

    <div class="container">
        <div class="row">
            <h2>Conta alterada com sucesso!</h2>
        </div>
        <div class="row">
            <h3>Clique @Html.ActionLink("Aqui", "Index", "Home") para ser redirecionado a tela principal.</h3>
        </div>

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    </div>
}


